{
  "name": "upward_boilerplate",
  "version": "2.0.0",
  "description": "Upward Next Boilerplate for coding web pages",
  "author": "UPWARD NEXT INC",
  "license": "ISC",
  "scripts": {
    "build": "yarn gulp build --smap=false",
    "build:app": "yarn gulp buildApp --smap=false",
    "clean": "yarn clean:node && yarn clean:docker && rm -rf dist",
    "clean:docker": "docker volume prune",
    "clean:node": "concurrently \"rm -rf node_modules\" \"rm -rf .cache\" \"rm -rf .parcel-cache\"",
    "close": "docker-compose down --remove-orphans",
    "close:delete": "docker-compose down --remove-orphans && docker volume prune",
    "dev": "yarn gulp dev",
    "dev:docker": "yarn gulp buildApp && docker-compose -f docker-compose.yml -f docker-compose-dev.yml up",
    "dev:docker:staging": "yarn gulp buildApp && docker-compose -f docker-compose.yml -f docker-compose-stg.yml up",
    "dev:server": "yarn gulp devApp",
    "dev:wp": "yarn dev:docker -d && yarn dev:server",
    "lint": "yarn gulp lint",
    "postinstall": "node initEnv.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.18.6",
    "@npmcli/fs": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "babelify": "^10.0.0",
    "browser-sync": "^2.27.10",
    "browserify": "^17.0.0",
    "concurrently": "^8.2.2",
    "eslint": "8.37.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-pug": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "pre-commit": "^1.2.2",
    "pug": "^3.0.2",
    "sass": "^1.58.3",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "tsify": "^5.0.4",
    "typescript": "5.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^17.7.2"
  },
  "precommit": {
    "run": "build"
  }
}
